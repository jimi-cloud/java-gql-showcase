type ToolType {
    id: ID,
    type: String
    dailyRentalCharge: Float
}
type ToolCode {
    id: ID,
    code: String
}
type Brand {
    id: ID,
    name: String
}
type Tool {
    id: ID,
    toolCode: ToolCode,
    toolType: ToolType,
    brand: Brand
}

type Query {
    findToolById(id: ID): Tool
    findAllTools: [Tool]
    findToolsByBrandId(brandId: ID): [Tool]
    findToolsByToolTypeId(toolTypeId: ID): [Tool]
}